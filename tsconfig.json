{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"module": "ESNext",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"@/*": ["./*"],
			"react-native": ["./node_modules/react-native"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
